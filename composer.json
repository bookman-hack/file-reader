{
  "name": "hhpack/file",
  "keywords": [
    "file", "utility", "simple", "easy", "hack"
  ],
  "description": "File utility library for Hack",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "hhvm": ">=3.12"
  },
  "require-dev": {
    "peridot-php/peridot": "~1.18",
    "peridot-php/peridot-dot-reporter": "~1.0",
    "expect/peridot-expect-plugin": "~3.0",
    "hhpack/typesafety": "~0.2"
  },
  "autoload": {
    "psr-4": {
      "hhpack\\file\\": "src/"
    },
    "files": [
      "src/bootstrap.hh"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "hhpack\\file\\example\\": "example/custom/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/peridot -c peridot.hh --grep *Spec.hh spec -r dot",
    "check:init": "touch .hhconfig",
    "check": "vendor/bin/typesafety"
  },
  "minimum-stability": "stable"
}
