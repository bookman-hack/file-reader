{
  "name": "hhpack/file",
  "keywords": [
    "file", "utility", "simple", "easy", "hack"
  ],
  "description": "File utility library for Hack",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "hhvm": ">= 3.21.0",
    "facebook/hhvm-autoload": "^1.4"
  },
  "require-dev": {
    "hackpack/hackunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "HHPack\\File\\": "src/"
    },
    "files": [
      "src/bootstrap.hh"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HHPack\\File\\Example\\": "example/custom/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/hackunit test",
    "check:init": "touch .hhconfig",
    "check": "./typecheck"
  },
  "minimum-stability": "stable"
}
